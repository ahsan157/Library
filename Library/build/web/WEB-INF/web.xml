<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>welcome_admin</servlet-name>
        <servlet-class>welcome_admin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>update_user</servlet-name>
        <servlet-class>update_user</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>delete_user</servlet-name>
        <servlet-class>delete_user</servlet-class>
